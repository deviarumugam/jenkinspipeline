    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Iam Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Iam Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Iam Testing'
            }
        }
    }
}
